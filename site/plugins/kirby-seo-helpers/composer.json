{
  "name": "johannschopplich/kirby-seo-helpers",
  "description": "Panel helpers for SEO and translations",
  "type": "kirby-plugin",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich",
  "authors": [
    {
      "name": "Johann Schopplich",
      "email": "pkg@johannschopplich.com",
      "homepage": "https://johannschopplich.com"
    }
  ],
  "require": {
    "getkirby/composer-installer": "^1.2",
    "deeplcom/deepl-php": "^1.6",
    "symfony/http-client": "^7.0",
    "nyholm/psr7": "^1.8"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true,
      "php-http/discovery": true
