{
  "require": {
    "getkirby/cms": "^3.9",
    "symfony/yaml": "^5.4",
    "johannschopplich/kirby-helpers": "^4.0",
    "johannschopplich/kirby-algolia-docsearch": "^0.2",
    "johannschopplich/kirby-punctuation-section": "^2.1",
    "bnomei/kirby3-feed": "^1.5",
    "tobimori/kirby-blurhash": "^1.1"
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8000 -t public vendor/getkirby/cms/router.php"
    ]
  },
  "autoload": {
    "psr-4": {
      "Kirby\\": "site/classes/Kirby/"
    },
    "exclude-from-classmap": [
      "vendor/getkirby/cms/src/Data/Yaml.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "kirby-cms-path": false
  }
}
