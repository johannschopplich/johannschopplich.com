{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "assets:head": "esbuild src/head.js --format=esm --target=es2020 --minify --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "format": "prettier \"src/**/*\" --write"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.5.0",
    "@docsearch/css": "^3.6.1",
    "@docsearch/js": "^3.6.1",
    "@iconify-json/bx": "^1.2.0",
    "@iconify-json/bxs": "^1.2.0",
    "@types/node": "^20.16.5",
    "@unocss/cli": "^0.62.3",
    "@unocss/eslint-plugin": "^0.62.3",
    "@unocss/preset-mini": "^0.62.3",
    "@unocss/preset-wind": "^0.62.3",
    "animere": "^3.0.0",
    "concurrently": "^9.0.0",
    "drauu": "^0.4.1",
    "duecss": "^0.32.0",
    "es-toolkit": "^1.17.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "medium-zoom": "^1.1.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "typescript": "^5.5.3",
    "unlazy": "^0.11.3",
    "unocss": "^0.62.3",
    "vite": "^5.4.3",
    "vite-plugin-full-reload": "^1.2.0"
  }
}
