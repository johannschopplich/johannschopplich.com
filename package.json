{
  "private": true,
  "packageManager": "pnpm@7.28.0",
  "scripts": {
    "assets:head": "esbuild src/head.js --minify --format=esm --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier \"src/**/*\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "@docsearch/css": "^3.3.3",
    "@docsearch/js": "^3.3.3",
    "@neodrag/vanilla": "^2.0.3",
    "animate.css": "^4.1.1",
    "animere": "^2.1.0",
    "destr": "^1.2.2",
    "drauu": "^0.3.2",
    "duecss": "^0.28.0",
    "loadeer": "^2.1.2",
    "medium-zoom": "^1.0.8",
    "swiper": "^9.1.0"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.6",
    "@iconify-json/bxs": "^1.1.6",
    "@iconify-json/fa-brands": "^1.1.4",
    "@nuxt/eslint-config": "^0.1.1",
    "@types/node": "^18.14.6",
    "@unocss/cli": "^0.50.3",
    "@unocss/preset-wind": "^0.50.3",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.11",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "typescript": "^4.9.5",
    "unocss": "^0.50.3",
    "vite": "^4.1.4",
    "vite-plugin-full-reload": "^1.0.5"
  }
}
