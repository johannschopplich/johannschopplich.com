{
  "private": true,
  "packageManager": "pnpm@7.24.3",
  "scripts": {
    "assets:head": "esbuild src/head.ts --minify --format=iife --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier \"src/**/*\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "animere": "^1.18.1",
    "drauu": "^0.3.2",
    "duecss": "^0.27.2",
    "loadeer": "^2.1.1",
    "medium-zoom": "^1.0.8"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.5",
    "@iconify-json/bxs": "^1.1.5",
    "@iconify-json/fa-brands": "^1.1.3",
    "@nuxt/eslint-config": "^0.1.1",
    "@types/node": "^18.11.18",
    "@unocss/cli": "^0.48.3",
    "@unocss/preset-wind": "^0.48.3",
    "concurrently": "^7.6.0",
    "esbuild": "^0.16.17",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "unocss": "^0.48.3",
    "vite": "^4.0.4",
    "vite-plugin-full-reload": "^1.0.5"
  }
}
