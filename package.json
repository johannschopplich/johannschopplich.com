{
  "prrivate": true,
  "scripts": {
    "assets:head": "esbuild src/inline-head.ts --minify --format=iife --outdir=public/dist/assets",
    "assets:worker": "esmo scripts/generateServiceWorker.ts",
    "assets:unocss": "esmo scripts/generateUnoCSS.ts",
    "dev": "shx touch src/.lock && concurrently \"npm run assets:unocss\" \"vite\"",
    "build": "shx rm -f src/.lock && NODE_ENV=production npm run assets:unocss && vite build && npm run assets:head && npm run assets:worker",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@github/mini-throttle": "^2.1.0",
    "@splidejs/splide": "^3.2.6",
    "animate.css": "^4.1.1",
    "animere": "^1.14.1",
    "drauu": "^0.2.1",
    "medium-zoom": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@unocss/preset-icons": "^0.6.6",
    "colorette": "^2.0.16",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "esmo": "^0.12.0",
    "fast-glob": "^3.2.7",
    "nanoid": "^3.1.30",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "shx": "^0.3.3",
    "unocss": "^0.6.6",
    "vite": "^2.6.13",
    "vite-plugin-full-reload": "^0.2.2"
  }
}
