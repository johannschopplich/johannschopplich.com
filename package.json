{
  "private": true,
  "scripts": {
    "assets:head": "esbuild src/head.ts --minify --format=iife --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "shx touch src/.lock && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "shx rm -f src/.lock && npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.{css,scss,ts}\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "animere": "^1.17.0",
    "drauu": "^0.3.0",
    "duecss": "^0.25.9",
    "loadeer": "^1.3.3",
    "medium-zoom": "^1.0.6"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.2",
    "@iconify-json/bxs": "^1.1.2",
    "@iconify-json/fa-brands": "^1.1.1",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@unocss/cli": "^0.31.3",
    "@unocss/core": "^0.31.3",
    "@unocss/preset-wind": "^0.31.3",
    "concurrently": "^7.1.0",
    "esbuild": "^0.14.36",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "shx": "^0.3.4",
    "typescript": "^4.6.3",
    "unocss": "^0.31.3",
    "vite": "^2.9.5",
    "vite-plugin-full-reload": "^1.0.0"
  }
}
