{
  "private": true,
  "packageManager": "pnpm@8.6.5",
  "scripts": {
    "assets:head": "esbuild src/head.js --minify --format=esm --target=es2020 --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier \"src/**/*\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "@docsearch/css": "^3.5.1",
    "@docsearch/js": "^3.5.1",
    "animere": "^2.1.1",
    "destr": "^2.0.0",
    "drauu": "^0.3.2",
    "duecss": "^0.30.0",
    "medium-zoom": "^1.0.8",
    "swiper": "^9.4.1",
    "unlazy": "^0.9.0"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.6",
    "@iconify-json/bxs": "^1.1.6",
    "@iconify-json/fa-brands": "^1.1.4",
    "@nuxt/eslint-config": "^0.1.1",
    "@types/node": "^18.16.18",
    "@unocss/cli": "^0.53.4",
    "@unocss/preset-wind": "^0.53.4",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.10",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "fontaine": "^0.4.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "typescript": "^5.1.6",
    "unocss": "^0.53.4",
    "vite": "^4.3.9",
    "vite-plugin-full-reload": "^1.0.5"
  }
}
