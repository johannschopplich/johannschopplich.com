{
  "private": true,
  "scripts": {
    "assets:head": "esbuild src/head.ts --minify --format=iife --outdir=public/dist/assets",
    "assets:worker": "node scripts/generateServiceWorker.mjs",
    "assets:unocss": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/vendor/uno.css",
    "dev": "shx touch src/.lock && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "shx rm -f src/.lock && npm run assets:unocss && vite build && npm run assets:head && npm run assets:worker",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@splidejs/splide": "^3.6.9",
    "animate.css": "^4.1.1",
    "animere": "^1.16.3",
    "drauu": "^0.2.1",
    "duecss": "^0.16.10",
    "medium-zoom": "^1.0.6"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.452",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@unocss/cli": "^0.21.0",
    "@unocss/preset-icons": "^0.21.0",
    "concurrently": "^7.0.0",
    "consola": "^2.15.3",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "nanoid": "^3.1.30",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "shx": "^0.3.3",
    "typescript": "^4.5.4",
    "unocss": "^0.21.0",
    "vite": "^2.7.10",
    "vite-plugin-full-reload": "^1.0.0"
  }
}
