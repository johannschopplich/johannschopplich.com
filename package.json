{
  "private": true,
  "packageManager": "pnpm@7.16.1",
  "scripts": {
    "assets:head": "esbuild src/head.ts --minify --format=iife --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.{css,scss,ts}\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "animere": "^1.18.0",
    "drauu": "^0.3.2",
    "duecss": "^0.27.0",
    "loadeer": "^2.1.0",
    "medium-zoom": "^1.0.8"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.4",
    "@iconify-json/bxs": "^1.1.4",
    "@iconify-json/fa-brands": "^1.1.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@unocss/cli": "^0.46.5",
    "@unocss/preset-wind": "^0.46.5",
    "concurrently": "^7.5.0",
    "esbuild": "^0.15.14",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "typescript": "^4.9.3",
    "unocss": "^0.46.5",
    "vite": "^3.2.4",
    "vite-plugin-full-reload": "^1.0.4"
  }
}
