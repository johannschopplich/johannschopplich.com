{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "assets:head": "esbuild src/head.js --format=esm --target=es2020 --minify --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "format": "prettier \"src/**/*\" --write"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@byjohann/utils": "^0.7.1",
    "@docsearch/css": "^3.6.2",
    "@docsearch/js": "^3.6.2",
    "@iconify-json/bx": "^1.2.1",
    "@iconify-json/bxs": "^1.2.1",
    "@types/node": "^20.17.1",
    "@unocss/cli": "^0.63.6",
    "@unocss/core": "^0.63.6",
    "@unocss/eslint-plugin": "^0.63.6",
    "@unocss/preset-mini": "^0.63.6",
    "@unocss/preset-wind": "^0.63.6",
    "animere": "^3.0.0",
    "concurrently": "^9.0.1",
    "drauu": "^0.4.1",
    "es-toolkit": "^1.26.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "medium-zoom": "^1.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "sass": "^1.80.4",
    "typescript": "^5.6.3",
    "unlazy": "^0.11.3",
    "unocss": "^0.63.6",
    "vite": "^5.4.10",
    "vite-plugin-full-reload": "^1.2.0"
  }
}
