{
  "private": true,
  "packageManager": "pnpm@8.7.0",
  "scripts": {
    "assets:head": "esbuild src/head.js --minify --format=esm --target=es2020 --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier \"src/**/*\" --write"
  },
  "dependencies": {
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "animere": "^2.1.1",
    "destr": "^2.0.1",
    "drauu": "^0.3.5",
    "duecss": "^0.30.0",
    "medium-zoom": "^1.0.8",
    "swiper": "^10.2.0",
    "unlazy": "^0.9.2"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.7",
    "@iconify-json/bxs": "^1.1.7",
    "@iconify-json/fa-brands": "^1.1.5",
    "@nuxt/eslint-config": "^0.2.0",
    "@types/node": "^20.5.7",
    "@unocss/cli": "^0.55.3",
    "@unocss/preset-wind": "^0.55.3",
    "concurrently": "^8.2.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "typescript": "^5.1.6",
    "unocss": "^0.55.3",
    "vite": "^4.4.9",
    "vite-plugin-full-reload": "^1.0.5"
  }
}
