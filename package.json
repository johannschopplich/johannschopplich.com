{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "assets:head": "esbuild src/head.js --format=esm --target=es2020 --minify --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "format": "prettier \"src/**/*\" --write"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@byjohann/utils": "^0.7.1",
    "@docsearch/css": "^3.7.0",
    "@docsearch/js": "^3.7.0",
    "@iconify-json/bx": "^1.2.1",
    "@iconify-json/bxs": "^1.2.1",
    "@types/node": "^22.9.0",
    "@unocss/cli": "^0.64.0",
    "@unocss/core": "^0.64.0",
    "@unocss/eslint-plugin": "^0.64.0",
    "@unocss/preset-mini": "^0.64.0",
    "@unocss/preset-wind": "^0.64.0",
    "animere": "^3.0.0",
    "concurrently": "^9.1.0",
    "drauu": "^0.4.1",
    "es-toolkit": "^1.27.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.14.0",
    "medium-zoom": "^1.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "typescript": "^5.6.3",
    "unlazy": "^0.11.7",
    "unocss": "^0.64.0",
    "vite": "^5.4.11",
    "vite-plugin-full-reload": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "function-bind": "npm:@socketregistry/function-bind@^1",
      "hasown": "npm:@socketregistry/hasown@^1",
      "indent-string": "npm:@socketregistry/indent-string@^1",
      "is-core-module": "npm:@socketregistry/is-core-module@^1",
      "path-parse": "npm:@socketregistry/path-parse@^1"
    }
  }
}
