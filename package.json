{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "inline": "esbuild src/inline/*.js --minify --outdir=public/dist/inline",
    "dev": "rm -rf public/dist && mprocs",
    "build": "unocss && vite build && pnpm run inline",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "blocks:extract": "node scripts/content-extractor/extract.ts",
    "blocks:inject": "node scripts/content-extractor/inject.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@csstools/postcss-global-data": "^4.0.0",
    "@iconify-json/ri": "^1.2.8",
    "@iconify-json/tabler": "^1.2.26",
    "@types/alpinejs": "^3.13.11",
    "@types/node": "^24.10.9",
    "@unocss/cli": "^66.6.0",
    "@unocss/core": "^66.6.0",
    "@unocss/eslint-plugin": "^66.6.0",
    "@unocss/preset-mini": "^66.6.0",
    "@unocss/preset-wind3": "^66.6.0",
    "@unocss/preset-wind4": "^66.6.0",
    "@unocss/reset": "^66.6.0",
    "alpinejs": "^3.15.5",
    "concurrently": "^9.2.1",
    "consola": "^3.4.2",
    "culori": "^4.0.2",
    "drauu": "^1.0.0",
    "embla-carousel": "^9.0.0-rc01",
    "es-toolkit": "^1.44.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "mprocs": "^0.8.3",
    "postcss": "^8.5.6",
    "postcss-custom-media": "^12.0.0",
    "postcss-import": "^16.1.1",
    "postcss-load-config": "^6.0.1",
    "postcss-nesting": "^14.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "unlazy": "^1.1.0",
    "unocss": "^66.6.0",
    "vite": "^7.3.1",
    "vite-plugin-full-reload": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "function-bind": "npm:@socketregistry/function-bind@^1.0.7",
      "hasown": "npm:@socketregistry/hasown@^1.0.7",
      "indent-string": "npm:@socketregistry/indent-string@^1.0.14",
      "is-core-module": "npm:@socketregistry/is-core-module@^1.0.11",
      "path-parse": "npm:@socketregistry/path-parse@^1.0.8",
      "shell-quote": "npm:shell-quote@^1.8.3"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
