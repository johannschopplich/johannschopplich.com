{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "scripts": {
    "assets:head": "esbuild src/head.js --format=esm --target=es2020 --minify --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/**/*\" --write"
  },
  "dependencies": {
    "@docsearch/css": "^3.6.0",
    "@docsearch/js": "^3.6.0",
    "animere": "^3.0.0",
    "destr": "^2.0.3",
    "drauu": "^0.4.0",
    "duecss": "^0.32.0",
    "medium-zoom": "^1.1.0",
    "unlazy": "^0.11.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@iconify-json/bx": "^1.1.10",
    "@iconify-json/bxs": "^1.1.10",
    "@types/node": "^20.12.12",
    "@unocss/cli": "^0.60.2",
    "@unocss/eslint-plugin": "^0.60.2",
    "@unocss/preset-mini": "^0.60.2",
    "@unocss/preset-wind": "^0.60.2",
    "concurrently": "^8.2.2",
    "esbuild": "^0.21.3",
    "eslint": "^9.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.77.1",
    "typescript": "^5.4.5",
    "unocss": "^0.60.2",
    "vite": "^5.2.11",
    "vite-plugin-full-reload": "^1.1.0"
  }
}
