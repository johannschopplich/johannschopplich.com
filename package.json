{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "assets:head": "esbuild src/head.js --format=esm --target=es2022 --minify --outdir=public/dist/assets",
    "dev": "rm -rf public/dist && concurrently \"unocss --watch\" \"vite\"",
    "build": "unocss && vite build && npm run assets:head",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "format": "prettier \"src/**/*\" --write"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@iconify-json/bx": "^1.2.2",
    "@iconify-json/bxs": "^1.2.2",
    "@types/node": "^24.3.1",
    "@unocss/cli": "^66.5.1",
    "@unocss/core": "^66.5.1",
    "@unocss/eslint-plugin": "^66.5.1",
    "@unocss/preset-mini": "^66.5.1",
    "@unocss/preset-wind3": "^66.5.1",
    "@unocss/preset-wind4": "^66.5.1",
    "@unocss/reset": "^66.5.1",
    "concurrently": "^9.2.1",
    "drauu": "^0.4.3",
    "es-toolkit": "^1.39.10",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "medium-zoom": "^1.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "typescript": "^5.9.2",
    "unlazy": "^0.12.4",
    "unocss": "^66.5.1",
    "vite": "^7.1.5",
    "vite-plugin-full-reload": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "function-bind": "npm:@socketregistry/function-bind@^1.0.7",
      "hasown": "npm:@socketregistry/hasown@^1.0.7",
      "indent-string": "npm:@socketregistry/indent-string@^1.0.13",
      "is-core-module": "npm:@socketregistry/is-core-module@^1.0.10",
      "path-parse": "npm:@socketregistry/path-parse@^1.0.7"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
