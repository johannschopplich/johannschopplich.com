{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "assets:head": "esbuild src/head.js --format=esm --target=es2022 --minify --outdir=public/dist/assets",
    "dev": "rm -rf public/dist && mprocs",
    "build": "unocss && vite build && npm run assets:head",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "blocks:extract": "tsx scripts/content-extractor/extract.ts",
    "blocks:inject": "tsx scripts/content-extractor/inject.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.1",
    "@csstools/postcss-global-data": "^3.1.0",
    "@iconify-json/ri": "^1.2.6",
    "@iconify-json/tabler": "^1.2.23",
    "@types/alpinejs": "^3.13.11",
    "@types/node": "^24.10.4",
    "@unocss/cli": "^66.5.10",
    "@unocss/core": "^66.5.10",
    "@unocss/eslint-plugin": "^66.5.10",
    "@unocss/preset-mini": "^66.5.10",
    "@unocss/preset-wind3": "^66.5.10",
    "@unocss/preset-wind4": "^66.5.10",
    "@unocss/reset": "^66.5.10",
    "alpinejs": "^3.15.3",
    "concurrently": "^9.2.1",
    "consola": "^3.4.2",
    "drauu": "^0.4.3",
    "es-toolkit": "^1.43.0",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.2",
    "medium-zoom": "^1.1.0",
    "mprocs": "^0.8.2",
    "postcss": "^8.5.6",
    "postcss-custom-media": "^11.0.6",
    "postcss-import": "^16.1.1",
    "postcss-load-config": "^6.0.1",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unlazy": "^1.0.0",
    "unocss": "^66.5.10",
    "vite": "^7.2.7",
    "vite-plugin-full-reload": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "function-bind": "npm:@socketregistry/function-bind@^1.0.7",
      "hasown": "npm:@socketregistry/hasown@^1.0.7",
      "indent-string": "npm:@socketregistry/indent-string@^1.0.14",
      "is-core-module": "npm:@socketregistry/is-core-module@^1.0.11",
      "path-parse": "npm:@socketregistry/path-parse@^1.0.8"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
