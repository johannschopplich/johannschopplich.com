{
  "private": true,
  "scripts": {
    "assets:head": "esbuild src/head.ts --minify --format=iife --outdir=public/dist/assets",
    "assets:worker": "node scripts/generateServiceWorker.mjs",
    "assets:unocss": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/vendor/uno.css",
    "dev": "shx touch src/.lock && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "shx rm -f src/.lock && npm run assets:unocss && vite build && npm run assets:head && npm run assets:worker",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.{css,scss,ts}\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "@splidejs/splide": "^3.6.12",
    "animate.css": "^4.1.1",
    "animere": "^1.16.3",
    "drauu": "^0.2.1",
    "duecss": "^0.17.1",
    "medium-zoom": "^1.0.6"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.0",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@unocss/cli": "^0.24.4",
    "concurrently": "^7.0.0",
    "consola": "^2.15.3",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.21",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "fast-glob": "^3.2.11",
    "nanoid": "^3.3.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "shx": "^0.3.4",
    "typescript": "^4.5.5",
    "unocss": "^0.24.4",
    "vite": "^2.8.3",
    "vite-plugin-full-reload": "^1.0.0"
  }
}
