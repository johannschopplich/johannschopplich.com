{
  "private": true,
  "packageManager": "pnpm@7.5.0",
  "scripts": {
    "assets:head": "esbuild src/head.ts --minify --format=iife --outdir=public/dist/assets",
    "assets:unocss": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "shx touch src/.lock && concurrently \"npm run assets:unocss -- --watch\" \"vite\"",
    "build": "shx rm -f src/.lock && npm run assets:unocss && vite build && npm run assets:head",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.{css,scss,ts}\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "animere": "^1.18.0",
    "drauu": "^0.3.0",
    "duecss": "^0.26.0",
    "loadeer": "^2.0.1",
    "medium-zoom": "^1.0.6"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.3",
    "@iconify-json/bxs": "^1.1.3",
    "@iconify-json/fa-brands": "^1.1.2",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@unocss/cli": "^0.44.0",
    "@unocss/core": "^0.44.0",
    "@unocss/preset-wind": "^0.44.0",
    "concurrently": "^7.2.2",
    "esbuild": "^0.14.49",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "shx": "^0.3.4",
    "typescript": "^4.7.4",
    "unocss": "^0.44.0",
    "vite": "^3.0.0-beta.9",
    "vite-plugin-full-reload": "^1.0.1"
  }
}
