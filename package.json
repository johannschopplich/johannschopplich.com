{
  "scripts": {
    "assets:head": "esbuild src/js/inline-head.js --minify --format=iife --outdir=public/dist/assets",
    "assets:worker": "node scripts/buildServiceWorker.js",
    "dev": "shx touch src/.lock && vite",
    "build": "shx rm -f src/.lock && vite build && npm run assets:head && npm run assets:worker",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "@github/mini-throttle": "^2.1.0",
    "@splidejs/splide": "^3.1.7",
    "animate.css": "^4.1.1",
    "animere": "^1.14.1",
    "drauu": "^0.2.1",
    "medium-zoom": "^1.0.6"
  },
  "devDependencies": {
    "colorette": "^2.0.16",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "fast-glob": "^3.2.7",
    "nanoid": "^3.1.30",
    "prettier": "^2.4.1",
    "sass": "^1.43.2",
    "shx": "^0.3.3",
    "vite": "^2.6.10"
  }
}
